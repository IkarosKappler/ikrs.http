package ikrs.http;

/**
 * @author Ikaros Kappler
 * @date 2012-10-11
 * @version 1.0.0
 **/

public class UnsupportedMethodException 
    extends HTTPException  {

    /**
     * Creates a new UnsupportedVersionException witht the given error message.
     **/
    public UnsupportedMethodException( String msg ) {
	super( msg );
    }


}