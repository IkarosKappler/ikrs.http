package ikrs.httpd;

/**
 * @author Ikaros Kappler
 * @date 2012-07-16
 * @version 1.0.0
 **/

public class HTTPException 
    extends Exception {

    private int generatedStatusCode;
    
    private String generatedReasonPhrase;
    
    public HTTPException( String msg,
			  
			  int statusCode,
			  String reasonPhrase ) {
	super( msg );
    }

    public HTTPException( String msg,
			  
			  int statusCode,
			  String reasonPhrase,
			  
			  Throwable reason ) {
	super( msg, reason );
    }

}